{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "rootDir": "./resources/js",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@/*": ["./resources/js/*"],
      "@/components/*": ["./resources/js/components/*"],
      "@/hooks/*": ["./resources/js/hooks/*"],
    }
  }
}
